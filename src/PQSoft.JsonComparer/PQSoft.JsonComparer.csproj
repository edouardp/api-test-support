<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AssemblyName>PQSoft.JsonComparer</AssemblyName>
    <PackageId>PQSoft.JsonComparer</PackageId>
    <Authors>PQSoft</Authors>
    <Description>A powerful library for comparing JSON documents with token support and function execution.</Description>
    <PackageTags>json;comparison;testing;api</PackageTags>
    <RepositoryUrl>https://github.com/your-repo/PQSoft</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Text.Json" Version="9.0.0" />
  </ItemGroup>

</Project>